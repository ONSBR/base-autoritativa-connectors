{
  "name": "mapa-informacao-connector",
  "version": "0.1.0",
  "description": "Wrapper for queries on Mapa da Informação system, based on Neo4J",
  "main": "dist",
  "scripts": {
    "test": "mocha --recursive --reporter spec --timeout 3000"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coichedid/mapa-informacao-connector.git"
  },
  "keywords": [
    "connector",
    "neo4j",
    "nodejs"
  ],
  "author": "Clovis Chedid",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/coichedid/mapa-informacao-connector/issues"
  },
  "homepage": "https://github.com/coichedid/mapa-informacao-connector#readme",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^3.1.1",
    "mocha": "^3.3.0",
    "nodemon": "^1.9.2",
    "sinon": "^2.2.0"
  },
  "dependencies": {
    "node-rest-client": "^3.1.0"
  }
}
